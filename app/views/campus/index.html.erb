<p id="notice"><%= notice %></p>

<h1>Campus</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Nome</th>
      <th>Numero</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Cep</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campus.each do |campu| %>
      <tr>
        <td><%= campu.address %></td>
        <td><%= campu.nome %></td>
        <td><%= campu.numero %></td>
        <td><%= campu.bairro %></td>
        <td><%= campu.cidade %></td>
        <td><%= campu.cep %></td>
        <td><%= link_to 'Show', campu %></td>
        <td><%= link_to 'Edit', edit_campu_path(campu) %></td>
        <td><%= link_to 'Destroy', campu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campu', new_campu_path %>
